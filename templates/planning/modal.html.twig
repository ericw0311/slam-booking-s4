<div id="bnaBefore" class="modal fade" role="dialog">
<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<h4 class="modal-title">{{ 'booking.not.allowed'|trans }}</h4>
		</div>
		<div class="modal-body">
			<p>{{ 'booking.bna.before'|trans({'%firstBookingDate%': dateCourte(planningContext.firstBookingDate)}) }}</p>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal">{{ 'close'|trans }}</button>
		</div>
	</div>
</div>
</div>

<div id="bnaAfter" class="modal fade" role="dialog">
<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<h4 class="modal-title">{{ 'booking.not.allowed'|trans }}</h4>
		</div>
		<div class="modal-body">
			<p>{{ 'booking.bna.after'|trans({'%lastBookingDate%': dateCourte(planningContext.lastBookingDate)}) }}</p>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal">{{ 'close'|trans }}</button>
		</div>
	</div>
</div>
</div>
